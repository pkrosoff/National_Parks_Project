    "# read data and rename column to be deleted later\n",
    "parks_data = pd.read_csv(\"../data/np_data.csv\")\n",
    "parks_data.rename(columns={\"area_km2\":\"AREA_km2\"},inplace=True)"
   ]
  },
  {
   "cell_type": "code",
